        position: relative;
        top: 10px;
        margin: 10px;
        overflow-y: scroll;
        padding: 5px;
        /* height: calc(100% - 10px); */

    }
    #purchase-cart, #shopping-cart {
        padding: 5px;
    }
    .display h3 {
        text-align: left;
        left: 0px;
    }
    /* .profile {
        height: 2100px;
    } */
    .display input {
        width: 100%;
    }
}
#footer {
    position: relative;
    top: 200px;
    bottom: 0px;
}

.botnav {
    list-style-type: none;
    margin: 0;
    max-width: 100vw;
