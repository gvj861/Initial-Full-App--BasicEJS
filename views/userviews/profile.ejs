<style>
* {
    margin: 0;
    padding: 0;
    background-color: rgb(13, 31, 34);  
    /* overflow-y: scroll; */
}

.topnav {
    list-style-type: none;
    margin: 0;
    max-width: 100vw;
    padding: 5px;
    overflow: hidden;
    background-color: #075E54;
}

 .topnav a {
    float: left;
    display: block;
    color: white;
    text-align: center;
    padding: 10px 16px;
    text-decoration: none;
}

.topnav a:hover {
    color: white;
    font-size: 17px;
    background-color: #0d312d;
    text-decoration: none;
}

.topnav a.right {
    float: right;
}

.topnav a.icon {
    display: none;
}
@media screen and (max-width: 500px) {
    .topnav a:not(:first-child) { display: none;}
    .topnav a.icon {
        float: right;
        display: block;
    }
    
}
#logo {
    position: relative;
    max-width: 100px;
    /* left: 45%; */
    padding: 5px;
    margin: 5px 10px 5px 10px;
    cursor: pointer;
    background-color: #075E54;
}
@media screen and (max-width: 590px) {
    .topnav.responsive {position: relative;}
    .topnav.responsive .icon {
      position: absolute;
      right: 0;
      top: 0;
    }
    .topnav.responsive a {
      float: none;
      display: block;
      text-align: left;
    }
    .botnav {
        display: flex;
        height: 150px;
    }
    #logo {
        width: 100px;
        height: 60px;
    }
    .botnav a {
        font-size: 12px;
    }
  }
.profile {
    position: relative;
    max-width: 1100px;
    height: 1300px;
    margin: auto;
    top: 80px;
    box-shadow: 2px 2px 30px 1px gray;
}
.buttons{
    padding: 0px;
    width: 260px;
    background-color: #075E54;
    height: 1300px;
    list-style-type: none;
}
.buttons i {
    font-size: 30px;
    padding: 5px 10px;
    /* background-color: #075E54; */
}
.buttons a {
    display: block;
    font-size: 20px;
    border-right: 1px solid white;
    color: rgb(255, 255, 255);
    padding: 16px 30px;
    text-decoration: none;
}

.buttons a:hover{
    background-color: #075E54;
    
    letter-spacing: 1px;
    color: rgb(255, 255, 255);
}
.buttons a:hover i {
    background-color: #075E54;
}
.display {
    position: absolute;
    padding: 20px 20px;
    top: 10px;
    overflow-y: scroll;
    height: calc(100% - 40px);
    
    width: calc(100% - 290px);
    margin-left: 270px;
    /* margin-bottom: 200px; */
    /* border: 2px solid white; */
    /* border: 1px solid; */
}
.display #account,
#changepassword {
    /* border: 1px solid; */
    padding: 20px;
    margin: 10px;
    /* margin-bottom: 50px; */
}

#account h3,
#changepassword h3 {
    color: white;
}
#profilepic {
    position: relative;
    max-width: 300px;
    margin-right: 0px;
    left: 10%;
}
#profilepic img {
    width: 190px;
    height: auto;
    
    border-radius: 100px;
}
 #account i,
 #changepassword i {
    position: absolute;
    font-size: 28px;
    margin: 10px;
    color: black;
    background-color: white;
}
#account p,
#changepassword p {
    color: white;
    position: relative;
    margin-top: 23px;
    margin-bottom: 2px;
    max-width: 200px;
    height: auto;
    font-size: 20px;
    font-family: sans-serif, 'Times New Roman', Times, serif;
}
#account input,
#changepassword input {
    margin-top: 2px;
    background-color: white;
    border-radius: 6px;
    /* width: calc(80% - 20px); */
    width: 100%;
    padding: 10px 50px;
}
#profilepic input {
    max-width: 300px;
    background-color: rgb(13, 31, 34);
    color: white;
    padding: 10px;
}
#uploaduserphoto, 
#savepassword {
    background-color: white;
    color: #075E54; 
    width: 170px;    
    padding: 5px;
    border-radius: 5px;
    cursor: pointer;
    outline: none;
}
#uploaduserphoto:hover, 
#savepassword:hover {
    border: 2px solid white;
    background-color: #075E54;
    color: white;
    letter-spacing: 1px;
}
.shipping #savesettings {
    position: relative;
    margin: 10px;
    top: -30px;
}
@media screen and (max-width: 600px) {
    .buttons {
        width: 100%;
        height: auto;
        position: relative;
    }
    .buttons a {
        float: left;
    }
    .display {
        width: calc(100% - 20px);
        position: relative;
        top: 100px;
        margin: auto;
        height: auto;
    }
    .display h3 {
        left: 0px;
    }
    .account img {
        float: left;
    }
    #profilepic {
        left: 0px;
    }
    #profilepic img {
        border-radius: 100px;
        border: 1px solid;
        width: 125px;
        height: 140px;
    }
}
@media screen and (max-width: 500px) {
    .buttons a {
        width: auto;
        text-align: center;
        float: none;
    }
